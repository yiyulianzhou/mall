<{include file="../public/header.html" }>
<!-- /main navbar --> 
<!-- Page container -->
<div class="page-container"> 
  
  <!-- Page content -->
  <div class="page-content">
      <{include file="../public/left.html" }>
    <div class="content-wrapper">
      <!-- Content area -->
      <div class="content" ng-controller="appCtrl">
        <div class="panel panel-flat">

          <div class="row">
            <div class=" col-md-12">
              <div class="panel panel-flat">
                <div class="panel-heading">
                  <h6 class="panel-title">今日数据<span class="pull-right"><{date('Y-m-d')}></span></h6>
                </div>
                <div class="container-fluid">
                  <div class="row col-md-offset-1">
                    <div class="col-md-3">
                      <div class="panel bg-teal-400">
                        <div class="panel-body pb20 homgicon-01">
                          <h4 class="no-margin">卖家提现总额</h4>
                          <h2 class="mv10">{{lists.totalMoney}}元</h2>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-3 col-md-offset-1">
                      <div class="panel bg-pink-400 ">
                        <div class="panel-body pb20 homgicon-02">
                          <h4 class="no-margin">卖家提现总次数</h4>
                          <h2 class="mv10">{{lists.totalTimes}}次</h2>
                        </div>
                      </div>
                    </div>
                    <div class="col-md-3 col-md-offset-1">
                      <div class="panel bg-blue-400">
                        <div class="panel-body pb20 homgicon-03">
                          <h4 class="no-margin">卖家提现总人数</h4>
                          <h2 class=" mv10">{{lists.totalPeople}}人</h2>
                        </div>
                      </div>
                    </div>
                  </div>
                </div>

              </div>
            </div>
          </div>
          <div class="row">
            <div class="panel panel-info col-md-6">

              <div class="panel-heading" >
                <div class="panel-title row">
                  <div class="col-md-6 pt10">提现金额统计
                    <button class="btn">今日</button>
                    <button class="btn">本周</button>
                    <button class="btn">本月</button>
                  </div>
                  <div class="col-md-4 col-sm-4 m-ss" style="color:#666">
                    指定日期
                    <div class="input-group"> <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                      <input type="text" name="s_start_time" ng-model="s_start_time" class="form-control pickadate"  placeholder="请选择时间">
                    </div>
                    <div class="input-group"> <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                      <input type="text" name="s_end_time" ng-model="s_end_time" class="form-control pickadate"  placeholder="请选择时间">
                    </div>

                  </div>
                  <div class="col-md-2 pt20">
                    <button class="btn btn-success">筛选</button>
                  </div>
                </div>

              </div>
              <div class="panel-body br-t" >
                <div class="chart-container">
                  <div class="chart has-fixed-height" id="stacked_lines"></div>
                </div>
              </div>
            </div>
            <div class="panel panel-info col-md-6">
              <div class="panel-heading">
                <div class="panel-title row">
                  <div class="col-md-6 pt10">
                    提现卖家统计
                    <button class="btn">今日</button>
                    <button class="btn">本周</button>
                    <button class="btn">本月</button>
                  </div>
                  <div class="col-md-4 col-sm-4 m-ss" style="color:#666">
                    指定日期
                    <div class="input-group"> <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                      <input type="text" name="s_start_time" ng-model="s_start_time" class="form-control pickadate"  placeholder="请选择时间">
                    </div>
                    <div class="input-group"> <span class="input-group-addon"><i class="icon-calendar3"></i></span>
                      <input type="text" name="s_end_time" ng-model="s_end_time" class="form-control pickadate"  placeholder="请选择时间">
                    </div>

                  </div>
                  <div class="col-md-2 pt20">
                    <button class="btn btn-success">筛选</button>
                  </div>
                </div>

              </div>
              <div class="panel-body br-t" >
                <div class="chart-container">
                  <div class="chart has-fixed-height" id="stacked2_lines"></div>
                </div>
              </div>
            </div>
          </div>
          <div class="panel-heading">
            <h5 class="panel-title">卖家提现<span class="ml10">
              <input name="" type="button"  value="<{$searchbtn}>"  class="btn btn-xs bg-grey-100 text-xg flitbtn" id="searchbtn" onclick="btn()" >
              </span></h5>
            <div class="heading-elements"></div>
          </div>
          <form action="" method="GET" class="form-container">
            <div class="panel-body br-t pt20" id="searchbox" style="<{$searchbox}>">
              <div class="row">
                <div class="col-md-2 col-sm-4 m-ss">
                  <input type="text" class="form-control" placeholder="卖家姓名" name="username" value="<{$username}>">
                </div>
                <div class="col-md-2 col-sm-6 m-ss">
                  <select name="state" class="form-control">
                    <option value="-1"<{if $state eq -1}> selected="selected"<{/if}>>提现状态</option>
                    <{foreach from=$cashState item=cashstate key=key}>
                    <option value="<{$key}>"<{if $state eq $key}> selected="selected"<{/if}>><{$cashstate}></option>
                    <{/foreach}>
                  </select>
                </div>
                <div class="col-md-1 m-ss">
                  <input type="submit" class=" btn btn-block bg-grey-300" value="筛选">
                </div>
              </div>
            </div>
          </form>
          <table class="table datatable-basic table-hover">
            <thead class="bg-grey-100">
              <tr>
                <th>卖家姓名</th>
                <th>头像</th>
                <th>提现金额</th>
                <th>提现时间</th>
                <th>操作人</th>
                <th>操作时间</th>
                <th>打款人</th>
                <th>打款时间</th>
                <th>状态</th>
                <{if isset($permission_tree.verify) || isset($permission_tree.pay) || isset($permission_tree.detail) }>
                <th class="text-center">操作</th>
                <{/if}>
              </tr>
            </thead>
            <tbody><!-- danger -->
              <{foreach from=$sellerCash item=v}>
              <tr>
                <td><{$v.username}></td>
                <td> <img src="<{$v.avatarUrl}>" style="width:60px;height:60px;"></td>
                <td><{$v.money}></td>
                <td><{date('Y-m-d H:i:s',$v.create_time)}></td>
                <td><{$v.realname}></td>
                <td><{if !empty($v.verify_time)}><{date('Y-m-d H:i:s',$v.verify_time)}><{/if}></td>
                <td><{$v.payName}></td>
                <td><{if !empty($v.pay_time)}><{date('Y-m-d H:i:s',$v.pay_time)}><{/if}></td>
                <td><{($v.status == '1')?'待审核':(($v.status == '3')?'审核未通过':(($v.status =='12')?'待打款':'提现完成'))}></td>
                <{if isset($permission_tree.verify) || isset($permission_tree.pay) || isset($permission_tree.detail) }>
                <td class="text-center">
                  <ul class="icons-list">
                    <li class="dropdown"> <a href="#" class="dropdown-toggle" data-toggle="dropdown"> <i class="icon-menu9"></i> </a>
                      <ul class="dropdown-menu dropdown-menu-right mnw100">
                        <{if isset($permission_tree.verify) || isset($permission_tree.pay) || isset($permission_tree.detail) }>
                        <{if isset($permission_tree.verify) && $v.status == '1'}>
                        <li><a href='<{site_url("money/detail?id=<{$v.id}>")}>&type=1&per_page=<{$page}>&state=<{$state}>&username=<{$username}>' ><i class="icon-compose"></i> 审核</a> 
                        </li>
                        <{elseif  isset($permission_tree.pay) && $v.status == '12'}>
                        <li><a href='<{site_url("money/detail?id=<{$v.id}>")}>&type=1&per_page=<{$page}>&state=<{$state}>&username=<{$username}>' ><i class="icon-compose"></i> 打款</a> 
                        </li>
                        <{elseif  isset($permission_tree.detail) }>
                        <li><a href='<{site_url("money/detail?id=<{$v.id}>")}>&type=1' ><i class="icon-compose"></i> 详情</a> 
                        </li>
                        <{/if}>
                        <{/if}>
                      </ul>
                    </li>
                  </ul>
                </td>
                <{/if}>
              </tr>
              <{/foreach}>
            </tbody>
          </table>
          <{include file="../public/pager.html"}>
        </div>

        <{include file="../public/footer_page.html" }>
        
      </div>
      <!-- /content area --> 
      
    </div>
    <!-- /main content --> 
    
  </div>
  <!-- /page content --> 
  
</div>
</div>
<!-- /page container --> 
<!-- /page JS files -->
<script type="text/javascript" src="<{$base_url}>assets/js/pages/modals_option.js"></script>
<script type="text/javascript" src="<{$base_url}>assets/js/plugins/forms/styling/uniform.min.js"></script>
<script type="text/javascript" src="<{$base_url}>assets/js/plugins/forms/styling/switch.min.js"></script>

<script type="text/javascript">
  var btn1=document.getElementById('searchbtn');
  var box1=document.getElementById('searchbox');
  function btn(){ 
    if(btn1.value=="筛选 －"){
      box1.style.display='none';
      btn1.value="筛选 ＋";
    }else{
      box1.style.display='';
      btn1.value="筛选 －";
    }
  }
</script>
<{*这里只加载echats库文件，图表配置项在assets/dev/pages/report/index.js中*}>
<script type="text/javascript" src="<{$base_url}>assets/js/plugins/visualization/echarts/echarts.js"></script>
<!-- 日期范围选择配置 -->
<script type="text/javascript" src="<{$base_url}>assets/js/pages/picker_date.js"></script>
<{include file="../public/footer.html" }>
