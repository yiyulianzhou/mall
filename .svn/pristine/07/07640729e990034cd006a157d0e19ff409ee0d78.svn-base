// JavaScript Document
$(document).ready(function(){
    $('input[type="radio"]').click(function(){
        if($(this).attr("value")=="stype1"){
            $(".hidebox1").not(".showbox1").hide();
            $(".showbox1").show();
        }
        if($(this).attr("value")=="stype2"){
            $(".hidebox1").not(".showbox2").hide();
            $(".showbox2").show();
        }
        if($(this).attr("value")=="stype3"){
            $(".hidebox1").not(".showbox3").hide();
            $(".showbox3").show();
        }
		 if($(this).attr("value")=="stype4"){
            $(".hidebox1").not(".showbox4").hide();
            $(".showbox4").show();
        }
		 if($(this).attr("value")=="stype5"){
            $(".hidebox1").not(".showbox5").hide();
            $(".showbox5").show();
        }
		 
    });
});

